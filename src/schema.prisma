datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GlobalSettings {
  version       Int      @id @default(autoincrement())
  discordToken  String
  defaultPrefix String?
  colorDefault  Int?
  colorBad      Int?
  colorGood     Int?
  colorWarn     Int?
  cleanInterval Int?
  masterUserIds String[] @db.VarChar(20)
}
