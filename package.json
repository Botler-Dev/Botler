{
  "name": "botler",
  "version": "0.0.1",
  "description": "Exon multipurpose discord bot",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "build:prod": "tsc",
    "build:dev": "tsc --sourceMap",
    "build:watch": "yarn build:dev --sourceMap",
    "start:prod": "node ./dist/main.js",
    "start:dev": "node --require dotenv/config --nolazy ./dist/main.js",
    "format": "npx prettier --write .",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Botler-Dev/Botler.git"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "author": "Codec, Montori",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Botler-Dev/Botler/issues"
  },
  "homepage": "https://github.com/Botler-Dev/Botler#readme",
  "engines": {
    "node": ">=15"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dayjs": "^1.9.3",
    "discord.js": "^12.3.1",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unicorn": "^23.0.0",
    "prettier": "^2.1.2",
    "prettier-airbnb-config": "^1.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
